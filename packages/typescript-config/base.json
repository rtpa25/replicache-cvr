{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Default",
  "compilerOptions": {
    "composite": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "inlineSources": false,
    "isolatedModules": true,
    "noUnusedParameters": true,
    "preserveWatchOutput": false,
    "skipLibCheck": true,
    "noUncheckedIndexedAccess": true,
    "strict": true
  }
}
