{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["./.env"],
  "globalDotEnv": ["./.env"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
